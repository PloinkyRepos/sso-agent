{
  "container": "postgres:16",
  "about": "PostgreSQL database backing services for Keycloak SSO.",
  "agent": "docker-entrypoint.sh postgres",
  "cli": "psql -h 127.0.0.1 -p ${POSTGRES_PORT:-5432} -U ${POSTGRES_USER:-keycloak}",
  "env": [
    "POSTGRES_DB",
    "POSTGRES_USER",
    "POSTGRES_PASSWORD"
  ],
  "ports": [
    { "name": "db", "container": 5432, "protocol": "tcp", "primary": true }
  ]
}
